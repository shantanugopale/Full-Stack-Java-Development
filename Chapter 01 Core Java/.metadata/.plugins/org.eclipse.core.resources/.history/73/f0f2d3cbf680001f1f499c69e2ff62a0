class Calculator
{
	int add(int a, int b)
	{
		return a + b;
	}
	
	int add(int a, int b, int c)
	{
		return a + b + c;
	}

	int add(int a, float b)
	{
		return a + b;
	}
	
	int add(float a, float b)
	{
		return a + b;
	}
	
	int add(int a, float b, float c)
	{
		return a + b + c;
	}
	
	int add(int a, int b, double c)
	{
		return a + b + c;
	}
	
	int add(double a, double b, double c)
	{
		return a + b + c;
	}
	
	int add(int a, double b, int c)
	{
		return a + b + c;
	}
	
}
public class C1_MethodOverloadingExample {

	public static void main(String[] args) {
		
		int a = 10, b = 20, c = 30;
		float m = 10.5f, n = 20.11f, o = 34.4f;
		double x = 15.5,y = 25.5,z = 35.5;
		
		Calculator obj = new Calculator();
		obj.add(a, b)
		obj.add(a, b, c)
	}

}
